{
  "name": "real-nft-bot",
  "version": "0.1.0",
  "description": "Telegram bot for NFT",
  "type": "module",
  "imports": {
    "#root/*": "./build/src/*"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "typecheck": "tsc",
    "build": "tsc --noEmit false",
    "dev": "npm --prefix src/frontend run build && tsc-watch --onSuccess \"tsx ./src/main.ts\"",
    "start": "tsc && tsx ./src/main.ts",
    "start:force": "tsx ./src/main.ts",
    "prepare": "npx husky install",
    "update": "npx npm-check-updates -u && cd src/frontend && npx npm-check-updates -u"
  },
  "author": "deptyped <deptyped@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@grammyjs/auto-chat-action": "0.1.1",
    "@grammyjs/auto-retry": "^2.0.1",
    "@grammyjs/hydrate": "1.4.1",
    "@grammyjs/i18n": "1.0.2",
    "@grammyjs/menu": "^1.2.2",
    "@grammyjs/parse-mode": "1.10.0",
    "@grammyjs/types": "3.10.0",
    "@pinata/sdk": "^2.1.0",
    "@tma.js/init-data-node": "^1.3.1",
    "@ton/core": "^0.56.3",
    "@ton/crypto": "^3.2.0",
    "@ton/ton": "^13.11.2",
    "@typegoose/typegoose": "^12.5.0",
    "@types/node-rsa": "^1.1.4",
    "axios": "^1.7.2",
    "callback-data": "1.1.1",
    "chatgpt": "^5.2.5",
    "dotenv": "16.4.5",
    "fastify": "4.28.1",
    "form-data": "^4.0.0",
    "grammy": "1.26.0",
    "grammy-guard": "0.5.0",
    "iso-639-1": "3.1.2",
    "markdown-table-ts": "^1.0.3",
    "mongoose": "~8.4.4",
    "node-fetch": "^3.3.2",
    "node-graceful-shutdown": "1.1.5",
    "node-rsa": "^1.1.1",
    "pino": "9.2.0",
    "pino-pretty": "11.2.1",
    "sharp": "^0.33.4",
    "tonweb": "^0.0.66",
    "tsx": "4.16.0",
    "znv": "0.4.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "22.1.0",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "eslint": "^8",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-unicorn": "54.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "prettier-plugin-organize-imports": "4.0.0",
    "tsc-watch": "6.2.0",
    "typescript": "5.5.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  }
}
